{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "default": {
            "()": "uvicorn.logging.DefaultFormatter",
            "fmt": "%(levelprefix)s %(message)s",
            "use_colors": null
        },
        "access": {
            "()": "uvicorn.logging.AccessFormatter",
            "fmt": "%(levelprefix)s %(client_addr)20s - '%(request_line)s' %(status_code)s",
            "use_colors": null
        },
        "default_file": {
            "()": "uvicorn.logging.DefaultFormatter",
            "fmt": "%(asctime)s %(levelname)-8s : %(filename)20s:%(lineno)4d - %(message)s",
            "use_colors": false
        },
        "access_file": {
            "()": "uvicorn.logging.AccessFormatter",
            "fmt": "%(asctime)s %(levelname)-8s : %(client_addr)20s - '%(request_line)s' %(status_code)s",
            "use_colors": false
        }
    },
    "handlers": {
        "default": {
            "formatter": "default",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stderr"
        },
        "access": {
            "formatter": "access",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stderr"
        },
        "default_file": {
            "formatter": "default_file",
            "class": "logging.handlers.TimedRotatingFileHandler",
            "filename": "logs/default.log",
            "when": "midnight",
            "backupCount": 7,
            "encoding": "utf-8"
        },
        "access_file": {
            "formatter": "access_file",
            "class": "logging.handlers.TimedRotatingFileHandler",
            "filename": "logs/access.log",
            "when": "midnight",
            "backupCount": 7,
            "encoding": "utf-8"
        }
    },
    "loggers": {
        "uvicorn": {
            "handlers": [
                "default", "default_file"
            ],
            "level": "INFO",
            "propagate": false
        },
        "uvicorn.error": {
            "level": "INFO"
        },
        "uvicorn.access": {
            "handlers": [
                "access", "access_file"
            ],
            "level": "INFO",
            "propagate": false
        }
    }
}
